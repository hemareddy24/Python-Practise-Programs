# Enter your code here. Read input from STDIN. Print output to STDOUT
def example(A, B):
    return A.issubset(B)
T=int(input())
for _ in range(T):
    N1=int(input())
    A=set(map(int, input().split()))
    N2=int(input())
    B=set(map(int, input().split()))
    ans=example(A, B)
    print(ans)
OR

if(A.intersection(B)==A):
    print(True)
else:
    print(False)
